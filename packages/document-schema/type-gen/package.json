{
  "name": "@palantir/pack.document-schema.type-gen",
  "version": "0.1.1",
  "description": "TypeScript type generation from document schema",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/pack.git"
  },
  "bin": {
    "type-gen-bun": "./bin/cli.ts",
    "type-gen": "./bin/cli-node.js"
  },
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "default": "./build/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf .turbo build dist lib test-output *.tgz tsconfig.tsbuildinfo",
    "cli": "pnpm exec ./bin/cli.ts",
    "lint": "eslint ./src && dprint check --config $(find-up dprint.json) --allow-no-files",
    "lint:fix": "eslint ./src --fix && dprint fmt --config $(find-up dprint.json) --allow-no-files",
    "test": "vitest run --passWithNoTests -u",
    "test:watch": "vitest --passWithNoTests",
    "transpileEsm": "monorepo-transpile -f esm -m normal -t node",
    "transpileTypes": "tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@osdk/client": "catalog:",
    "@osdk/foundry.pack": "catalog:",
    "@palantir/pack.document-schema.model-types": "workspace:~",
    "@palantir/pack.schema": "workspace:*",
    "bun": "catalog:",
    "commander": "catalog:",
    "conjure-client": "catalog:",
    "consola": "catalog:",
    "fs-extra": "catalog:",
    "js-yaml": "^4.1.1",
    "tiny-invariant": "catalog:",
    "yaml": "^2.8.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@palantir/pack.monorepo.tsconfig": "workspace:~",
    "@types/fs-extra": "catalog:",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "build"
  ],
  "main": "./build/esm/index.js",
  "types": "./build/types/index.d.ts",
  "type": "module"
}
