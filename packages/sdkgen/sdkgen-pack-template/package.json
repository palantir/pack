{
  "name": "@palantir/pack.sdkgen.pack-template",
  "version": "0.0.4-beta.1",
  "description": "PACK SDK template for generating types and Zod schemas from YAML",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/pack.git"
  },
  "exports": {
    ".": {
      "browser": "./build/browser/index.js",
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "require": "./build/cjs/index.js",
      "default": "./build/browser/index.js"
    },
    "./*": {
      "browser": "./build/browser/public/*.js",
      "import": {
        "types": "./build/types/public/*.d.ts",
        "default": "./build/js/public/*.js"
      },
      "require": "./build/cjs/public/*.js",
      "default": "./build/browser/public/*.js"
    }
  },
  "scripts": {
    "build": "pnpm transpile",
    "clean": "rimraf .turbo build dist lib test-output *.tgz tsconfig.tsbuildinfo",
    "lint": "eslint ./src && dprint check --config $(find-up dprint.json) --allow-no-files",
    "lint:fix": "eslint ./src --fix && dprint fmt --config $(find-up dprint.json) --allow-no-files",
    "test": "vitest run --passWithNoTests -u",
    "test:watch": "vitest --passWithNoTests",
    "transpile": "tsc",
    "transpileBrowser": "monorepo-transpile -f esm -m bundle -t browser",
    "transpileCjs": "monorepo-transpile -f cjs -m bundle -t node",
    "transpileEsm": "tsc",
    "transpileTypes": "tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@palantir/pack.document-schema.type-gen": "workspace:*",
    "fs-extra": "^11.3.2"
  },
  "devDependencies": {
    "@palantir/pack.monorepo.tsconfig": "workspace:~",
    "@palantir/pack.sdkgen": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "catalog:",
    "consola": "^3.2.3",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build/",
    "template/",
    "template.config.js"
  ],
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/cjs/index.d.ts",
  "type": "module"
}
