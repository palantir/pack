{
  "name": "@demo/canvas.app",
  "private": true,
  "version": "0.1.0",
  "description": "Canvas demo application",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/pack.git"
  },
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rimraf .turbo build dist lib test-output *.tgz tsconfig.tsbuildinfo",
    "dev": "vite",
    "lint": "eslint ./src && dprint check --config $(find-up dprint.json) --allow-no-files",
    "lint:fix": "eslint ./src --fix && dprint fmt --config $(find-up dprint.json) --allow-no-files",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests -u",
    "test:watch": "vitest --passWithNoTests",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@blueprintjs/core": "^6.4.1",
    "@blueprintjs/icons": "^6.3.2",
    "@demo/canvas.sdk": "workspace:*",
    "@types/rbush": "^4.0.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "rbush": "^4.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-router": "^7.10.1"
  },
  "devDependencies": {
    "@demo/canvas.sdk": "workspace:*",
    "@osdk/api": "catalog:",
    "@osdk/client": "catalog:",
    "@osdk/oauth": "catalog:",
    "@palantir/pack.app": "workspace:*",
    "@palantir/pack.auth": "workspace:*",
    "@palantir/pack.core": "workspace:*",
    "@palantir/pack.document-schema.model-types": "workspace:*",
    "@palantir/pack.monorepo.tsconfig": "workspace:~",
    "@palantir/pack.state.core": "workspace:*",
    "@palantir/pack.state.demo": "workspace:*",
    "@palantir/pack.state.react": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "^5.1.2",
    "rimraf": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:",
    "vite": "^7.2.6",
    "vitest": "catalog:"
  },
  "type": "module"
}
